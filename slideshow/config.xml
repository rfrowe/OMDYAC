<?xml version="1.0" encoding="utf-8" ?>
<cu3er>
	<settings>

        <description>
            <defaults heading_font="Arial"/>
        </description>

        <auto_play>
          <defaults symbol="circular" time="7"/>
          <tweenIn />
          <tweenOut />
          <tweenOver />
      	</auto_play>

    	<prev_button>
			<defaults round_corners="5,5,5,5"/>
			<tweenOver tint="0xFFFFFF" scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</prev_button>

    	<prev_symbol>
			<tweenOver tint="0x000000" />
		</prev_symbol>

    	<next_button>
			<defaults round_corners="5,5,5,5"/>
			<tweenOver tint="0xFFFFFF"  scaleX="1.1" scaleY="1.1"/>
			<tweenOut tint="0x000000" />
		</next_button>

    	<next_symbol>
			<tweenOver tint="0x000000" />
		</next_symbol>

	</settings>

	<slides>

        <slide>
            <url>images/cover/cover.jpg</url>
        </slide>

        <transition num="3" slicing="vertical" direction="down" z_multiplier="1"/>

        <slide>
       		<url>images/cover/4.jpg</url>
            <description>
                <heading>Opening cirlce at the Social Action Con</heading>
            </description>
        </slide>

		<transition num="6" direction="up" delay="0.05" z_multiplier="2"/>

        <slide>
       		<url>images/cover/7.jpg</url>
        </slide>

        <transition num="5" direction="left" slicing="vertical" shader="none" z_multiplier="2" />

	</slides>
</cu3er>

